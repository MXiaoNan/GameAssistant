<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mn.gameassistant">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".module.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/qq"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".module.main.ui.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".module.main.ui.MainActivity">

        </activity>

        <activity android:name=".module.main.ui.RBActivity" />

        <activity android:name=".module.main.ui.RegActivity" />

        <activity android:name=".module.main.ui.LoginActivity" />

        <activity android:name=".module.main.ui.AgreeTextActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <data android:scheme="gameass" />
            </intent-filter>
        </activity>

        <activity android:name=".module.mygames.ui.MyGameActivity" />

    </application>

</manifest>